<sf12m>
	<!--Experimental map format for Space Fortress 12-->
	<!--Use only discrete integers for coordinates-->

	<station name = "Test Station 13">
	
		<turf x="3" y="2" z="1" type_text="girder">
			<overlay plating="grey_default" dir="2"></overlay>
			<overlay plating="grey_default" dir="8"></overlay>
			<overlay plating="grey_default" dir="32"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="2"></underlay>
			<underlay plating="grey_default" dir="8"></underlay>
			<underlay plating="grey_default" dir="32"></underlay>
		</turf>

		<turf x="3" y="2" z="2" type_text="girder">
			<overlay plating="grey_default" dir="8"></overlay>
			<overlay plating="grey_default" dir="32"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="8"></underlay>
			<underlay plating="grey_default" dir="32"></underlay>
		</turf>
		<turf x="3" y="2" z="3" type_text="girder" filling="grey_default">
			<overlay plating="grey_default" dir="8"></overlay>
			<overlay plating="grey_default" dir="32"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="8"></underlay>
			<underlay plating="grey_default" dir="32"></underlay>
		</turf>
		
		<turf x="2" y="1" z="1" type_text="girder">
			<overlay plating="grey_default" dir="16"></overlay>
			
			<underlay plating="grey_default" dir="16"></underlay>
		</turf>
		<turf x="2" y="1" z="2" type_text="girder">
			<overlay plating="grey_default" dir="16"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="16"></underlay>
		</turf>
		<turf x="2" y="1" z="3" type_text="girder">
			<overlay plating="grey_default" dir="16"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="16"></underlay>
		</turf>
		
		<turf x="1" y="2" z="1" type_text="girder" filling="grey_default">
			<overlay plating="grey_default" dir="4"></overlay>
			<overlay plating="grey_default" dir="32"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="4"></underlay>
			<underlay plating="grey_default" dir="32"></underlay>
		</turf>
		<turf x="1" y="2" z="2" type_text="girder">
			<overlay plating="grey_default" dir="4"></overlay>
			<overlay plating="grey_default" dir="32"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="4"></underlay>
			<underlay plating="grey_default" dir="32"></underlay>
		</turf>
		<turf x="1" y="2" z="3" type_text="girder">
			<overlay plating="grey_default" dir="1"></overlay>
			<overlay plating="grey_default" dir="4"></overlay>
			<overlay plating="grey_default" dir="32"></overlay>
			
			<obj tag="box"></obj>
			
			<underlay plating="grey_default" dir="4"></underlay>
			<underlay plating="grey_default" dir="1"></underlay>
			<underlay plating="grey_default" dir="32"></underlay>
		</turf>
		
		<playerspawn x="1" y="2" z="3" type="observer" dir="2"></playerspawn>
		
	</station>

</sf12m>
		